    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-app": "^7.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "cookie-parser": "^1.4.3",
